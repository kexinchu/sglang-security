{
    "trie_patterns": [
        {
            "name": "sensitive_words",
            "pattern": "password,secret,private,confidential,internal,classified,top_secret,restricted",
            "pattern_type": "trie",
            "severity": "medium",
            "description": "常见敏感词"
        },
        {
            "name": "company_secrets",
            "pattern": "trade_secret,proprietary,patent,unpublished,undisclosed,strategic",
            "pattern_type": "trie",
            "severity": "high",
            "description": "公司机密信息"
        },
        {
            "name": "financial_terms",
            "pattern": "revenue,profit,earnings,financial_statement,balance_sheet,income_statement",
            "pattern_type": "trie",
            "severity": "high",
            "description": "财务敏感信息"
        },
        {
            "name": "personal_info",
            "pattern": "ssn,social_security,passport,driver_license,credit_card,debit_card",
            "pattern_type": "trie",
            "severity": "critical",
            "description": "个人身份信息"
        }
    ],
    "regex_patterns": [
        {
            "name": "email",
            "pattern": "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b",
            "pattern_type": "regex",
            "severity": "high",
            "description": "邮箱地址"
        },
        {
            "name": "phone_cn",
            "pattern": "\\b1[3-9]\\d{9}\\b",
            "pattern_type": "regex",
            "severity": "high",
            "description": "中国手机号"
        },
        {
            "name": "phone_us",
            "pattern": "\\b\\d{3}-\\d{3}-\\d{4}\\b|\\b\\(\\d{3}\\)\\s*\\d{3}-\\d{4}\\b",
            "pattern_type": "regex",
            "severity": "high",
            "description": "美国电话号码"
        },
        {
            "name": "id_card_cn",
            "pattern": "\\b[1-9]\\d{5}(18|19|20)\\d{2}((0[1-9])|(1[0-2]))(([0-2][1-9])|10|20|30|31)\\d{3}[0-9Xx]\\b",
            "pattern_type": "regex",
            "severity": "critical",
            "description": "中国身份证号"
        },
        {
            "name": "ssn_us",
            "pattern": "\\b\\d{3}-\\d{2}-\\d{4}\\b",
            "pattern_type": "regex",
            "severity": "critical",
            "description": "美国社会安全号"
        },
        {
            "name": "bank_card",
            "pattern": "\\b\\d{16,19}\\b",
            "pattern_type": "regex",
            "severity": "high",
            "description": "银行卡号"
        },
        {
            "name": "credit_card",
            "pattern": "\\b(?:\\d{4}[\\s-]?){3}\\d{4}\\b",
            "pattern_type": "regex",
            "severity": "critical",
            "description": "信用卡号"
        },
        {
            "name": "ip_address",
            "pattern": "\\b(?:\\d{1,3}\\.){3}\\d{1,3}\\b",
            "pattern_type": "regex",
            "severity": "medium",
            "description": "IP地址"
        },
        {
            "name": "mac_address",
            "pattern": "\\b([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})\\b",
            "pattern_type": "regex",
            "severity": "medium",
            "description": "MAC地址"
        },
        {
            "name": "date_pattern",
            "pattern": "\\b\\d{4}-\\d{2}-\\d{2}\\b|\\b\\d{2}/\\d{2}/\\d{4}\\b",
            "pattern_type": "regex",
            "severity": "low",
            "description": "日期格式"
        },
        {
            "name": "url_pattern",
            "pattern": "\\bhttps?://[^\\s<>\"{}|\\\\^`\\[\\]]+\\b",
            "pattern_type": "regex",
            "severity": "low",
            "description": "URL地址"
        },
        {
            "name": "file_path",
            "pattern": "\\b[A-Za-z]:\\\\[^\\s<>\"{}|\\\\^`\\[\\]]+\\b|\\b/[^\\s<>\"{}|\\\\^`\\[\\]]+\\b",
            "pattern_type": "regex",
            "severity": "medium",
            "description": "文件路径"
        }
    ]
}